{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "arrowFunctions": true,
    "modules": true,
    "classes": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "jsx": true,
    "templateStrings": true,
    "spread": true,
    "destructuring" : true
  },
  "rules": {
    "semi": 2,
    "quotes": 0,
    "new-cap" : 0,

    "valid-jsdoc" : 0,
    "radix": 2,
  },
  "plugins": [
  ]
}
