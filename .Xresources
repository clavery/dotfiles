

Xft.dpi: 96
Xft.antialias: 1
Xft.hinting:    1
Xft.hintstyle:  hintfull
Xft.rgba:       rgb

xterm*faceName: xft:dejavu sans mono:size=12
!xterm*faceNameDoublesize:   xft:Droid Sans Japanese:style=Regular
xterm*dynamicColors:      true
xterm*utf8:               2
xterm*eightBitInput:      true
xterm*saveLines:          512
xterm*termName: screen-256color
*background: #1d1f21
*foreground: #C5c8c6

!black
*color0: #222222
*color8: #666666
!red
*color1: #E84F4F
*color9: #D23D3D
!green
*color2: #90AB76
*color10: #BDE077
!yellow
*color3: #FBB829
*color11: #FFE863
!blue
*color4: #657280
*color12: #AACCBB
!magenta
*color5: #B7416E
*color13: #E16A98
!cyan
*color6: #1693A5
*color14: #42717B
!white
*color7: #f6f6f6
*color15: #d4d4d4
