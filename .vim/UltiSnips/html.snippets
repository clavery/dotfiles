snippet bootstrap-form-horizontal "Bootstrap form horizontal" !b
<form class="form-horizontal" role="form">
  <div class="form-group">
    <label for="`!p snip.rv = t[1].lower().replace(' ', '-')`" class="col-sm-2 control-label">${1:label}</label>
    <div class="col-sm-10">
      <input type="${2:text email password}" class="form-control" id="`!p snip.rv = t[1].lower().replace(' ', '-')`" placeholder="$1">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn ${3:btn-default btn-primary}">${4:Submit}</button>
    </div>
  </div>
</form>

endsnippet

