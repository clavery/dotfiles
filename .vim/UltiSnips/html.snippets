snippet jquery "Jquery" b
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>

endsnippet

snippet angular "Bootstrap" b
<link href="https://netdna.bootstrapcdn.com/twitter-bootstrap/2.2.1/css/bootstrap-combined.min.css" rel="stylesheet">
<script src="https://netdna.bootstrapcdn.com/twitter-bootstrap/2.2.1/js/bootstrap.min.js"></script>

endsnippet

snippet angular "AngularJS" b
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.2/angular.min.js"></script>

endsnippet

snippet resource "AngularJS resource" b
<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.2/angular-resource.min.js"></script>

endsnippet

snippet mocks "AngularJS mocks" b
<script src="http://code.angularjs.org/1.0.2/angular-mocks.js"></script>

endsnippet

snippet table "XHTML <table>" !b
<table class="${1:table}">
  <thead>
    <tr><th>${2:Header}</th></tr>
  </thead>
  <tbody>
    <tr><td>${0:Data}</td></tr>
  </tbody>
</table>
endsnippet


snippet skel "Bootstrap HTML Skel" !b
<div class="container-fluid">
  <div class="navbar navbar-inverse">
    <div class="navbar-inner">
      <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </a>

      <a class="brand" href="#">${1:ProjectName}</a>

      <div class="nav-collapse collapse">
        <ul class="nav">
          <li class="active"><a href="#">${2:Link}</a></li>
          <!-- <li class="divider-vertical"></li> -->
        </ul>
      </div>
    </div>
  </div>
  <div class="row-fluid">
    <div class="${3:span12}">
      $0
    </div>
  </div>
</div>
endsnippet

snippet breadcrumb "Bootstrap Breadcrumbs" !b
<ul class="breadcrumb">
  <li><a href="/">${1:Home}</a> <span class="divider">/</span></li>
  <li><a href="${2:/next}">${3:Next}</a> <span class="divider">/</span></li>
  <li class="active">${4:Page}</li>
</ul>
endsnippet

snippet alert "Bootstrap Alert" !b
<div class="alert ${1:alert-error alert-success alert-info}">
  <button type="button" class="close" data-dismiss="alert">Ã—</button>
  <strong>${1:Warning}</strong> ${2:Content...}
</div>

endsnippet

snippet label "Boostrap Label" !b
<span class="label ${1:label-success label-warning label-important label-info label-inverse}">${2:content}</span>

endsnippet

snippet badge "Boostrap Badge" !b
<span class="badge ${1:badge-success badge-warning badge-important badge-info badge-inverse}">${2:10}</span>

endsnippet

snippet boilerplate "HTML5 Boilerplate" !b
